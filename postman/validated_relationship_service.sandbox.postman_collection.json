                      "// Validate if response header has matching content-type\npm.test(\"[PATCH]::/Consent/:id - Content-Type is application/fhir+json\", function () {\n   pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/fhir+json\");\n});\n",
                      "// Validate if response has JSON Body \npm.test(\"[PATCH]::/Consent/:id - Response has JSON Body\", function () {\n    pm.response.to.have.jsonBody();\n});\n"
                    ]
